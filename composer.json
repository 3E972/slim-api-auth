{
    "name": "jeremykendall/slim-auth",
    "description": "Authorization and authentication for the Slim Framework using ZF2 Authentication and Acl components",
    "homepage": "https://github.com/jeremykendall/slim-auth",
    "support": {
        "issues": "https://github.com/jeremykendall/slim-auth/issues",
        "source": "https://github.com/jeremykendall/slim-auth"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Kendall",
            "homepage": "http://about.me/jeremykendall",
            "role": "Developer"
        }
    ],
    "keywords": [
        "auth",
        "authorization",
        "authentication",
        "slim",
        "slim framework",
        "zend",
        "zend framework",
        "zf2"
    ],
    "bin": [
        "bin/version-check"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "jeremykendall/password-validator": "dev-develop",
        "wp-cli/php-cli-tools": "v0.9.4",
        "zendframework/zend-authentication": "2.*",
        "zendframework/zend-permissions-acl": "2.*",
        "zendframework/zend-session": "2.*"
    },
    "require-dev": {
        "league/phpunit-coverage-listener": "~1.1",
        "phpunit/phpunit": "3.*",
        "slim/slim": "2.*"
    },
    "autoload": {
        "psr-0": {
            "JeremyKendall\\Slim\\Auth\\": "src/"
        }
    },
    "suggest": {
        "zendframework/zend-db": "For the Zend\\Authentication\\Adapter\\DbTable adapters."
    }
}
